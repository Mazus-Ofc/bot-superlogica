NODE_ENV=production
PORT=3010

# WhatsApp
DEFAULT_WA_SESSION=whats-default

# Regras
HANDOFF_HOURS=12

# Superlógica
# api | email
SUPERLOGICA_MODE=api

# Se tiver endpoint oficial da sua licença para boletos:
# SUPERLOGICA_BASE_URL=https://apiassinaturas.superlogica.com
SUPERLOGICA_BASE_URL=
SUPERLOGICA_BOLETOS_PATH=/v1/boletos

# Para fallback por e-mail (usar seu subdomínio, ex. imob014312)
SUPERLOGICA_LICENSE=

# DB (preencher conforme seu ambiente)
PGHOST=localhost
PGPORT=5432
PGDATABASE=bot_boleto_whats
PGUSER=postgres
PGPASSWORD=postgres
